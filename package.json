{
  "name": "eslint-config-jnmorse",
  "version": "1.2.0",
  "description": "jnmorse Personal Eslint Configuration",
  "main": "index.js",
  "peerDependencies": {
    "eslint": ">= 4"
  },
  "scripts": {
    "lint": "eslint .",
    "current": "eslint-rule-finder --current",
    "unused": "eslint-rule-finder --unused",
    "lint-fix": "eslint . --fix",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "test": "npm run lint && npm run --silent unused"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/eslint-config-jnmorse.git"
  },
  "keywords": [
    "eslint",
    "config",
    "javascript"
  ],
  "author": "Joseph Morse <tamed.lionheart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnmorse/eslint-config-jnmorse/issues"
  },
  "homepage": "https://github.com/jnmorse/eslint-config-jnmorse#readme",
  "dependencies": {
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-restricted-globals": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-rule-finder": "^1.0.8"
  }
}
