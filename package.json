{
  "name": "eslint-config-jnmorse",
  "version": "2.2.2",
  "description": "jnmorse Personal Eslint Configuration",
  "main": "index.js",
  "peerDependencies": {
    "eslint": ">= 4"
  },
  "scripts": {
    "lint": "eslint .",
    "current": "eslint-rule-finder --current",
    "unused": "eslint-rule-finder --unused",
    "prerelease": "npm run lint && npm run test",
    "release": "standard-version --sign",
    "postrelease": "git push && git push --tags",
    "lint-fix": "eslint . --fix",
    "preversion": "npm run lint && npm run test",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/eslint-config-jnmorse.git"
  },
  "keywords": [
    "eslint",
    "config",
    "javascript"
  ],
  "author": "Joseph Morse <tamed.lionheart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnmorse/eslint-config-jnmorse/issues"
  },
  "homepage": "https://github.com/jnmorse/eslint-config-jnmorse#readme",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-restricted-globals": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-rule-finder": "^1.2.1",
    "jest": "^24.8.0",
    "standard-version": "^5.0.2"
  }
}
